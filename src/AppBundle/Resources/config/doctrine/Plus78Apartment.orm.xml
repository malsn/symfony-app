<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppBundle\Repository\Plus78ApartmentRepository" name="AppBundle\Entity\Plus78Apartment">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="xml" type="integer" column="xml_id" nullable="false" unique="true"/>
    <field name="section" type="integer" column="section" nullable="true"/>
    <field name="rooms" type="integer" column="rooms" nullable="true"/>
    <field name="stotal" type="float" column="stotal" nullable="true"/>
    <field name="sroom" type="float" column="sroom" nullable="true"/>
    <field name="skitchen" type="float" column="skitchen" nullable="true"/>
    <field name="sbalcony" type="float" column="sbalcony" nullable="true"/>
    <field name="scorridor" type="float" column="scorridor" nullable="true"/>
    <field name="swatercloset" type="string" column="swatercloset" length="255" nullable="true"/>
    <field name="height" type="float" column="height" nullable="true"/>
    <field name="flattypeid" type="integer" column="flattypeid"/>
    <field name="decoration" type="string" column="decoration" length="255" nullable="true"/>
    <field name="susidy" type="integer" column="susidy" nullable="true"/>
    <field name="creditend" type="integer" column="creditend" nullable="true"/>
    <field name="flatcostwithdiscounts" type="integer" column="flatcostwithdiscounts" nullable="true"/>
    <field name="baseflatcost" type="integer" column="baseflatcost" nullable="true"/>
    <field name="flatfloor" type="integer" column="flatfloor" nullable="true"/>
    <field name="dateadded" type="string" column="dateadded" length="255" nullable="true"/>
    <field name="datemodified" type="string" column="datemodified" length="255" nullable="true"/>
    <field name="flatplan" type="string" column="flatplan" length="255" nullable="true"/>
    <many-to-one target-entity="Plus78Block" field="block">
      <join-column name="block_id" referenced-column-name="xml" />
    </many-to-one>
    <many-to-one target-entity="Plus78Building" field="building">
      <join-column name="building_id" referenced-column-name="xml" />
    </many-to-one>
  </entity>
</doctrine-mapping>
