{% extends 'base.html.twig' %}

{% block body %}
    <div id="wrapper">
        <div id="container">
            <div id="welcome">
                <h1><span>Проданные квартиры:</span></h1>
                <table>
                    <thead>
                    <th>ID</th>
                    <th>ЖК</th>
                    <th>Тип квартиры</th>
                    <th>Базовая стоимость</th>
                    </thead>
                    {% for result in results %}
                    <tr>
                        <td>{{ result.xml }}</td>
                        <td>{{ result.block.name }}</td>
                        <td>{{ result.flattyp.name }}</td>
                        <td>{{ result.baseflatcost }}</td>
                    </tr>
                    {% endfor %}
                </table>
            </div>
        </div>
    </div>
{% endblock %}