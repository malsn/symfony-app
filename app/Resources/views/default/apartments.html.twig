{% extends 'base.html.twig' %}

{% block body %}
    <div id="wrapper">
        <div id="container">
            <div id="welcome">
                <h1><span>Проданные квартиры:</span></h1>
                <table>
                    <thead>
                    <th>ID Квартиры</th>
                    <th>Жилой Комлекс</th>
                    <th>Тип квартиры</th>
                    <th>Колиство комнат</th>
                    <th>Базовая стоимость, руб</th>
                    </thead>
                    {% for result in results %}
                    <tr>
                        <td>{{ result.xml }}</td>
                        <td>{{ result.block.name }}</td>
                        <td>{{ result.flattype.name }}</td>
                        <td>{{ result.rooms }}</td>
                        <td>{{ result.baseflatcost }}</td>
                    </tr>
                    {% endfor %}
                </table>
            </div>
        </div>
    </div>
{% endblock %}